length = input ("Please type Length : \n")
width  = input ("Please type Width  : \n")
the_price  = input ("how much for 1 meter  : \n")
length = float(length)
width  = float(width)
the_price  = float(the_price)
print ("the total area is")
print (length*width)
print ("Give the guy $: ")
print  ((length * width)* the_price)
 
